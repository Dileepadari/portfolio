  INSERT INTO public.profiles (user_id, email, full_name, is_admin)
  VALUES (
    NEW.id,
    NEW.email,
    COALESCE(NEW.raw_user_meta_data ->> 'full_name', NEW.email),
    NEW.email = 'rs200302@rguktsklm.ac.in' -- Make Dileep admin
  );
  RETURN NEW;
END;
$$;


